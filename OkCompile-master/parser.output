    7 VarDeclaration: TypeSpecifier VarDeclList ";"

    8 RecDeclaration: "record" ID "{" LocalDeclarations "}"

    9 ScopedVarDelaration: ScopedTypeSpecifer VarDeclList ";"

   10 VarDeclList: VarDeclList "," VarDeclInitialize
   11            | VarDeclInitialize

   12 VarDeclInitialize: VarDeclId
   13                  | VarDeclId ":" SimpleExpression

   14 VarDeclId: ID
